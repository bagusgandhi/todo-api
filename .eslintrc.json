{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-empty": "off",
        "no-irregular-whitespace":"off",
        "linebreak-style": "off",
        "no-param-reassign": "off",
        "no-unused-vars":"off",
        "camelcase": "off"
    }
}
